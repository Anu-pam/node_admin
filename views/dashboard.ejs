<style>
    .dropbtn {
      background-color: #191c20;
      color: white;
      padding: 16px;
      font-size: 16px;
      border: none;
    }
    
    .dropdown {
      position: relative;
      display: inline-block;
      
    }
    
    .dropdown-content {
      display: none;
      width: -webkit-fill-available;
       position: relative;
      /*position: absolute;*/
      background-color: #191c20;
      min-width: 200px;
      margin-left: -1px;
      box-shadow: 0px 8px 16px 0px rgba(0,0,0,0.2);
      z-index: 1;
      margin-top: -5px;
      border-radius:5px;
      
    }
    
    .dropdown-content a {
      color: #fff;
      padding: 12px 16px;
      text-decoration: none;
      display: block;
    }
    
    .dropdown-content a:hover {background-color: #2e2f32;}
    
    .dropdown:hover .dropdown-content {
        position: absolute;
       display: block;
        -webkit-animation: fadeIn 2s;
        animation: fadeIn 2s;
    }
    
    .dropdown:hover .dropbtn {background-color: #191c20;}
    </style>
     
  </head>
